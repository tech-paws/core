namespace TechPaws.Backend.Schemes;

enum ExecutionCommandType: byte {
    PushPos2f = 0,
    UpdateCameraPosition = 1,
}

struct Pos2f {
    x: float;
    y: float;
}

table ExecutionCommandData {
    pos2f: Pos2f;
}

table ExecutionCommand {
    type: ExecutionCommandType;
    data: ExecutionCommandData;
}

table ExecutionCommands {
    commands: [ExecutionCommand];
}

root_type ExecutionCommands;
